<div class="container">
  <h1>List details</h1>
  <h2><%= @list.name %></h2>
  <% @list.movies.each do |movie| %>
    <h2><%=movie.title%></h2>
    <p><%=movie.overview%></p>
    <p><%=movie.rating%></p>
<% end %>
   <p><%=@list.bookmarks.each do |b|%></p>
      <p><%=b.comment%></p>
  <% end %>
</div>
